{
  "editor.fontSize": 14,
  "editor.rulers": [
    80,
    120
  ],
  "editor.renderLineHighlight": "all",
  "editor.tabSize": 4,
  "editor.trimAutoWhitespace": true,
  "editor.tabCompletion": true,
  "explorer.openEditors.visible": 0,
  "window.restoreWindows": "all",
  "terminal.external.osxExec": "iTerm.app",
  "files.autoSave": "onWindowChange",
  "extensions.autoUpdate": true,
  "workbench.iconTheme": "eq-material-theme-icons-darker",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "extensions.ignoreRecommendations": false,
  "go.coverageDecorator": "gutter",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "/vendor": true
  },
  "workbench.startupEditor": "welcomePage",
  "editor.fontWeight": "300",
  "docker.showExplorer": false,
  "workbench.editor.showIcons": true,
  "go.testOnSave": false,
  "go.coverOnSave": false,
  "explorer.confirmDragAndDrop": false,
  "search.smartCase": true,
  "editor.fontFamily": "FuraCode Nerd Font, Fira Code,Ubuntu Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "bracketPairColorizer.showVerticalScopeLine": false,
  "bracketPairColorizer.showHorizontalScopeLine": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/Users/partha/.vscode/extensions/gabrielbb.vscode-lombok-0.9.6/server/lombok.jar\" -Xbootclasspath/a:\"/Users/partha/.vscode/extensions/gabrielbb.vscode-lombok-0.9.6/server/lombok.jar\"",
  "editor.formatOnSave": true,
  "javascript.validate.enable": false,
  "materialTheme.fixIconsRunning": false,
  "breadcrumbs.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "window.zoomLevel": 0,
  "workbench.fontAliasing": "auto",
}
